{
  "duplicate": {
    "value": true,
    "message": "Password is already in use",
    "customMessage": "{name} is already in use"
  },
  "min": {
    "value": 8,
    "message": "Password must be at least 8 characters long",
    "customMessage": "{name} must be at least {min} characters long"
  },
  "max": {
    "value": 256,
    "message": "Password must be less than 256 characters long",
    "customMessage": "{name} must be less than {max} characters long"
  },
  "required": {
    "value": true,
    "message": "Password is required",
    "customMessage": "{name} is required"
  },
  "regex": {
    "value": "^(?=.*[a-zA-Z\\d!\"#$%&'()*+,-./:;<=>?@[\\]^_`{|}~])[a-zA-Z\\d!\"#$%&'()*+,-./:;<=>?@[\\]^_`{|}~]{8,}$",
    "flags": "i",
    "message": "Password is invalid",
    "customMessage": "{name} is invalid"
  },
  "valid": {
    "value": true,
    "message": "Password is invalid",
    "customMessage": "{name} is invalid"
  },
  "replace": {
    "values": {
      "name": {
        "variable": "{name}",
        "default": "Password"
      },
      "min": {
        "variable": "{min}",
        "default": 8
      },
      "max": {
        "variable": "{max}",
        "default": 256
      }
    }
  }
}
